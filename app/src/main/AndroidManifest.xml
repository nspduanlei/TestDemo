            <intent-filter>
                <action android:name="com.dl.chapter_5.action.CLICK"/>
                <action android:name="android.appwidget.action.APPWIDGET_UPDATE"/>
            </intent-filter>
        </receiver>


        <receiver android:name=".four.MyReceiver">
